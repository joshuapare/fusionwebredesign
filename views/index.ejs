<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Create - Music App</title>
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.0/css/bootstrap.min.css" integrity="sha384-9aIt2nRpC12Uk9gS9baDl411NQApFmC26EwAOH8WgZl5MYYxFfc+NcPb1dKGj7Sk" crossorigin="anonymous">
    <link rel="stylesheet" type="text/css" href="../public/stylesheets/create.css">
    <script src="https://kit.fontawesome.com/023edff769.js" crossorigin="anonymous"></script></head>

    <script src="https://unpkg.com/wavesurfer.js"></script>
    
<body class="text-center">
    <div class="cover-container d-flex w-100 h-100 p-3 mx-auto flex-column">
    
        <!-- NAVBAR -->
    <header class="masthead">
        <div class="inner">
        <img src="../public/pictures/circle-logo.png" id="navbarlogo">  
          <h3 class="masthead-brand">Fusion Music App</h3>
          <nav class="nav nav-masthead justify-content-center">
            <a class="nav-link active" id="samples" href="#">Samples</a>
            <a class="nav-link" id="loops" href="#">Loops</a>
            <a class="nav-link" id="midi" href="#">MIDI</a>
            <a class="nav-link" id="multitracks" href="#">Multitracks</a>
            <a class="nav-link" id="presets" href="#">Presets</a>
            <a class="nav-link" id="plugins" href="#">Plugins</a>
            <a class="nav-link" href="#" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                <span class="user">Valerie Luna</span>
                <img class="img-profile rounded-circle" src="https://source.unsplash.com/QAB-WJcbgJk/60x60">
            </a>
          </nav>
          
        </div>
      </header>

        <!-- SIDEBAR -->
        <main role="main" class="inner cover">
            <div id="mainarea">
                <div class="row">
                    <div class="col-sm-3">
                        <ul class="list-group list-group-flush sidenavmain">
                            <li class="list-group-item sidenavitem">
                                <a class="nav-link sidenav" href="#"><i class="fas fa-user"></i>My Profile</a>
                            </li>
                            <li class="list-group-item sidenavitem">
                                <a class="nav-link sidenav" href="#"><i class="fas fa-plus"></i>Collections</a>
                            </li>
                            <li class="list-group-item sidenavitem">
                                <a class="nav-link sidenav" href="#"><i class="far fa-heart"></i>Favorites</a>
                            </li>
                            <li class="list-group-item sidenavitem">
                                <a class="nav-link sidenav" href="#"><i class="fas fa-folder"></i>Projects</a>
                            </li>
                            <li class="list-group-item sidenavitem">
                                <a class="nav-link sidenav" href="#"><i class="fas fa-arrow-circle-up"></i>Submit</a>
                            </li>
                           
                        </ul>
                        <ul class="list-group list-group-flush">
                            <li class="list-group-item">
                                <span>Download Queue</span>
                                <span>
                                    <i class="fas fa-arrow-circle-down fa-lg"></i>
                                    <i class="far fa-times-circle fa-lg"></i>
                                </span>
                            </li>
                            <table class="table table-dark download-queue">
                                <thead>

                                </thead>
                                <tbody>
                                    <tr>
                                        <td><i class="fas fa-play" onclick="wavesurfer.play()"></i></td>
                                        <td><img class="samplerart" src="../public/pictures/aaart1.jpeg"></td>
                                        <td>Sample 1</td>
                                    </tr>
                                </tbody>
                            </table>
                        </ul>
                    </div>
                    <div class="col-sm-9">
                    <!-- Dynamic Area -->
                        <div id="dyncontent"> 
                            
                        </div>

                    </div>
                </div>
            </div>
        </main>


        <!-- Footer -->
        <footer class="mastfoot mt-auto">
            <h6>&copy; Joshua Pare </h6>
        </footer>

    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script> 
